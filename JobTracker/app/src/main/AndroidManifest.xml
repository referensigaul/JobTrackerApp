<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.michael.jobtracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SeeJobsActivity" />
        <activity android:name=".AddJobActivity" />
        <activity android:name=".SeeEmployeesActivity" />
        <activity android:name=".AddEmployeeActivity" />
        <activity android:name="cz.destil.settleup.gui.MultiSpinner" />
        <activity android:name=".EditEmployee" />
        <activity android:name=".EditJob"></activity>
    </application>

</manifest>